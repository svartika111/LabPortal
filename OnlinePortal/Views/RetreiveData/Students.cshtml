@model IEnumerable<OnlinePortal.Models.Candidate>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>StudentRecords</title>
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <script src="~/scripts/jquery-3.3.1.min.js"></script>
    <script src="~/scripts/bootstrap.min.js"></script>
    <script src="~/scripts/GetData.js"></script>
    <style type="text/css">
        body {
            font-family: Arial;
            font-size: 10pt;
        }

        table {
            border: 5px solid #ccc;
            border-collapse: collapse;
            margin-top: 50px;
            margin-left: 50px;
        }

            table th {
                background-color: #F7F7F7;
                color: #333;
                font-weight: bold;
            }

            table th, table td {
                padding: 10px;
                padding-top: 20px;
                border: 1px solid #ccc;
            }

        .error {
            color: red;
        }
    </style>
</head>
<body>
    <h4>StudentRecords</h4>
    <hr />
    <table cellpadding="0" cellspacing="5" colspan="8">
        <tr>
            <th>FirstName</th>
            <th>LastName</th>
            <th>EmailID</th>
            <th>File</th>
            <th>CreatedOn</th>
        </tr>
        @foreach (OnlinePortal.Models.Candidate cseuser in Model)
        {
        <tr>
            <td>@cseuser.FirstName</td> &nbsp; &nbsp; &nbsp; &nbsp;
            <td>@cseuser.LastName</td> &nbsp; &nbsp; &nbsp; &nbsp;
            <td>@cseuser.EmailID</td>    &nbsp; &nbsp; &nbsp; &nbsp;
            <td>@cseuser.Resume</td> &nbsp;&nbsp; &nbsp;&nbsp;
            <td>@cseuser.CreatedOn</td> &nbsp; &nbsp;&nbsp; &nbsp;

        </tr>
        }
    </table>
</body>
</html>