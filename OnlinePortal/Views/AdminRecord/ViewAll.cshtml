@model IEnumerable<OnlinePortal.Models.Professor>
@{
    ViewBag.Title = "ViewAll";
    Layout = null;
}
<table class="table table-striped table-condensed" id="employeeTable">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Prof_ID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Prof_Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Username)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Password)
            </th>
            
            <th>
                Edit/Delete
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Prof_ID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Prof_Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Username)
            </td>


            <td>
                @Html.DisplayFor(modelItem => item.Password)
            </td>

            <td>
                <a class="btn btn-primary btn-sm" onclick="Edit('@Url.Action("AddOrEdit", "AdminRecord", new { id=@item.id})')"><i class="fa fa-pencil fa-lg"></i></a>
                <a class="btn btn-danger btn-sm" onclick="Delete('@Url.Action("Delete", "AdminRecord", new { id=@item.id})')"><i class="fa fa-trash fa-lg"></i></a>
            </td>
        </tr>
        }
    </tbody>
</table>
